    "bcryptjs": "^2.4.3",
    "body-parser": "^1.18.2",
    "express": "^4.16.2",
    "jsonwebtoken-promisified": "^1.0.3",
    "mongoose": "^4.12.3",
    "morgan": "^1.9.0"
  }
}
