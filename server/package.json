{
  "name": "server",
  "version": "1.0.0",
  "description": "Search All the Things",
  "main": "server.js",
  "directories": {
    "lib": "lib"
  },
  "scripts": {
    "lint": "eslint lib test",
    "pretest": "npm run lint",
    "test:mocha": "mocha  --recursive",
    "test": "npm run test:mocha",
    "test:unit": "mocha test/unit",
    "test:e2e": "mocha test/e2e",
    "test:watch": "nodemon --exec 'npm run test:mocha'",
    "test:e2e:watch": "nodemon --exec 'npm run test:e2e'",
    "test:unit:watch": "nodemon --exec 'npm run test:unit'",
    "start": "node server.js",
    "start:watch": "nodemon server.js"
  },
  "keywords": [],
  "author": "Keli Hansen",
  "license": "MIT",
  "devDependencies": {
    "chai": "^4.1.2",
    "chai-http": "^4.0.0",
    "eslint": "^4.19.1",
    "mocha": "^5.2.0",
    "nodemon": "^1.17.5"
  },
  "dependencies": {
    "bcryptjs": "^2.4.3",
    "dotenv": "^6.0.0",
    "express": "^4.16.3",
    "jsonwebtoken": "^8.3.0",
    "mongoose": "^5.1.5",
    "morgan": "^1.9.0",
    "superagent": "^3.8.3"
  }
}
